#include <iostream>
#include <chrono>

using namespace std;
using namespace std::chrono;

int nai(int n) 
{
    int count = 0;
  
    for (int k = 2; k <= n; k++) 
    {
        bool lp = true;
        for (int i = 2; i < k; i++) 
        {
            if (k % i == 0) 
            {
                lp = false;
                break;
            }
        }
        if (lp) count++;
    }
    return count;
}

int main() 
{
   auto start = high_resolution_clock::now();
  
    int n = 1000;  
    cout << nai(n) << endl;
  
  auto stop = high_resolution_clock::now();
  auto duration = duration_cast<microseconds>(stop - start);

  cout << duration.count() << endl;
  
    return 0;
}
